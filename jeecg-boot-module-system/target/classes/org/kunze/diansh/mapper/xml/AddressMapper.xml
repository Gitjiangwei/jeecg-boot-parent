<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.kunze.diansh.mapper.AddressMapper">

    <insert id="insertAddress" parameterType="org.kunze.diansh.entity.Address">
        insert into kz_address(id,user_id,consignee,consignee_sex,province,city,county,street,telphone,postcode,add_type,status)
        VALUES(#{id},#{userId},#{consignee},#{consigneeSex},#{province},#{city},#{county},#{street},#{telphone},#{postcode},#{addType},#{status})
    </insert>
</mapper>